#/bin/bash
mkdir bin
cp -r lost bin
find bin | grep -iE "\.pyc|\.py~|svn" | xargs rm -rdf
python /home/mlackman/PythonForS60/ensymble.py py2sis  --mode=pys60 --caps=Location+LocalServices+NetworkServices+ReadUserData+WriteUserData+UserEnvironment --heapsize=12k,4M --uid=0xe04584aa --extrasdir=extras --appname=lost --version=0.3.3 --caption=Lost bin/lost sis/lost_V0_33.sis
rm -rdf bin
